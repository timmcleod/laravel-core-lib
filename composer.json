{
    "name": "timmcleod/laravel-core-lib",
    "description": "A library for Laravel",
    "keywords": ["laravel"],
    "license": "MIT",
    "authors": [
        {
            "name": "Tim McLeod",
            "email": "tim@timmcleod.com"
        }
    ],
    "require": {
        "php": ">=7.3",
        "ramsey/uuid": "~3.0",
        "illuminate/support": "^7.0",
        "illuminate/filesystem": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5",
        "orchestra/testbench": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "TimMcLeod\\LaravelCoreLib\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable"
}
